{
  "name": "shared-firebase-functions",
  "version": "1.0.0",
  "description": "Shared Firebase Functions for BACKBONE Projects",
  "main": "lib/index.js",
  "types": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build-clipshow": "npx tsc src/clipShowPro/*.ts --outDir lib/clipShowPro --target es2020 --module commonjs --esModuleInterop --skipLibCheck",
    "build-timecards": "npx tsc src/timecards/*.ts --outDir lib/timecards --target es2018 --module commonjs --esModuleInterop --skipLibCheck",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-cloud/aiplatform": "^3.0.0",
    "@google-cloud/functions-framework": "^4.0.0",
    "@google/generative-ai": "^0.24.1",
    "@slack/oauth": "^3.0.4",
    "@slack/web-api": "^7.12.0",
    "airtable": "^0.12.2",
    "axios": "^1.6.0",
    "box-node-sdk": "^3.0.0",
    "crypto": "^1.0.1",
    "dropbox": "^10.34.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.1.0",
    "googleapis": "^134.0.0",
    "long": "^5.2.3",
    "nodemailer": "^6.10.1",
    "openai": "^4.0.0",
    "stripe": "^14.0.0",
    "docusign-esign": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.20",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "firebase",
    "functions",
    "shared",
    "backbone"
  ],
  "author": "BACKBONE Team",
  "license": "MIT"
}