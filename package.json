{
  "name": "shared-firebase-functions",
  "version": "1.0.0",
  "description": "Shared Firebase Functions for BACKBONE Projects",
  "main": "lib/index.js",
  "types": "lib/index.js",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "build": "tsc && rsync -a lib/shared-firebase-functions/src/ lib/ 2>/dev/null || true && rm -rf lib/shared-firebase-functions lib/shared-firebase-models",
    "build-clipshow": "npx tsc src/clipShowPro/*.ts --outDir lib/clipShowPro --target es2020 --module commonjs --esModuleInterop --skipLibCheck",
    "build-timecards": "npx tsc src/timecards/*.ts --outDir lib/timecards --target es2018 --module commonjs --esModuleInterop --skipLibCheck",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "seed:workflows": "ts-node scripts/seedWorkflows.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google-cloud/aiplatform": "^3.0.0",
    "@google-cloud/documentai": "^6.3.0",
    "@google-cloud/functions-framework": "^4.0.0",
    "@google-cloud/video-intelligence": "^5.3.0",
    "@google-cloud/vision": "^4.3.3",
    "@google/generative-ai": "^0.24.1",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@slack/oauth": "^3.0.4",
    "@slack/web-api": "^7.12.0",
    "@types/pdf-parse": "^1.1.5",
    "airtable": "^0.12.2",
    "axios": "^1.6.0",
    "box-node-sdk": "^3.0.0",
    "crypto": "^1.0.1",
    "docusign-esign": "^7.0.0",
    "dropbox": "^10.34.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^5.1.1",
    "googleapis": "^134.0.0",
    "jsonwebtoken": "^9.0.2",
    "ldapjs": "^3.0.7",
    "long": "^5.2.3",
    "mammoth": "^1.11.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "openai": "^4.0.0",
    "pdf-parse": "^2.4.5",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ldapjs": "^3.0.6",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.20",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "firebase",
    "functions",
    "shared",
    "backbone"
  ],
  "author": "BACKBONE Team",
  "license": "MIT"
}
